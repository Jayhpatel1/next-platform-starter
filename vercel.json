{
	"version": 2,
	"framework": "nextjs",
	"buildCommand": "npm run build",
	"outputDirectory": ".next",
	"images": {
		"domains": ["source.unsplash.com", "images.unsplash.com"]
	},
	"env": {
		"NEXT_PUBLIC_FIREBASE_API_KEY": "__REPLACE_ME__",
		"NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN": "superfood-ai.firebaseapp.com",
		"NEXT_PUBLIC_FIREBASE_PROJECT_ID": "superfood-ai",
		"NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET": "superfood-ai.appspot.com",
		"NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "000000000000",
		"NEXT_PUBLIC_FIREBASE_APP_ID": "1:000000000000:web:placeholder"
	}
}